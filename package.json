  "description": "",
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "csso-cli": "^4.0.2",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "sass": "^1.89.0"
  }
